{
    "name": "nexus/import",
    "description": "Framework-agnostic data import engine for Nexus ERP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nexus Development Team",
            "email": "dev@nexus.local"
        }
    ],
    "require": {
        "php": "^8.3",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Nexus\\Import\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\Import\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
